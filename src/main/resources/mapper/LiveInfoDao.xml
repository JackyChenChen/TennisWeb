<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chen.dao.LiveInfoDao">


    <!-- 目的：为dao接口方法提供sql语句配置 -->
    <select id="getLiveInfo" resultType="LiveInfo">
        <!-- 具体的sql -->
        SELECT
        *
        FROM
        live_info
        ORDER BY
        time
    </select>



</mapper>